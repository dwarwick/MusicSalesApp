{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SyncfusionTheme": "https://cdn.syncfusion.com/blazor/31.2.2/styles/bootstrap5.3",
  "Auth": {
    "ExpireMinutes": 300
  },
  "Fido2": {
    "ServerDomain": "streamtunes.net",
    "Origins": [ "https://streamtunes.net", "https://www.streamtunes.net" ],
    "TimestampDriftTolerance": 300000
  },  
  "PayPal": {
    "ClientId": "__REPLACE_WITH_PAYPAL_CLIENT_ID__",
    "Secret": "__REPLACE_WITH_PAYPAL_SECRET__",
    "ApiBaseUrl": "https://api-m.sandbox.paypal.com/", // replace with production https://api-m.paypal.com/ when going live
    "SubscriptionPrice": "3.99",
    "ReturnBaseUrl": "https://streamtunes.net", // Update this to match your application's base URL
    "PartnerId": "__REPLACE_WITH_PAYPAL_PARTNER_MERCHANT_ID__", // Your PayPal merchant ID (platform account)
    "BNCode": "__REPLACE_WITH_PAYPAL_BN_CODE__", // PayPal-Partner-Attribution-Id (BN Code) for partner tracking
    "WebhookId": "__REPLACE_WITH_PAYPAL_WEBHOOK_ID__" // Webhook ID for signature verification (created in PayPal Developer Dashboard)
  },
  "Facebook": {
    "AppId": "__REPLACE_WITH_FACEBOOK_APP_ID__"
  },
  "Supabase": {
    "SUPABASE_URL": "__REPLACE_WITH_SUPABASE_URL__",
    "SUPABASE_KEY": "__REPLACE_WITH_SUPABASE_KEY__"
  },
  "OpenAI": {
    "ApiKey": "__REPLACE_WITH_OPENAI_API_KEY__"
  }
}
